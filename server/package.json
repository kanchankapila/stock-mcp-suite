    "chromadb": "^3.0.14",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "hnswlib-node": "^3.0.0",
    "langchain": "^0.2.18",
    "node-fetch": "^3.3.2",
